{
  "name": "Emote",
  "key": "",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Emote Emotional Logging App",
  "background": {
    "scripts": ["oauth2/oauth2.js", "scripts/jquery-3.2.1.min.js", "background.js"],
    "persistent": true
  },
  "content_scripts": [{
    "matches": ["https://*/*", "http://*/*"],
    "js": ["scripts/mark.js", "scripts/jquery-3.2.1.min.js", "scripts/tether.min.js", "scripts/bootstrap.min.js", "content.js"],
    "css": ["buttoncss.css", "hlcss.css"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": [
    "images/menu1.jpg",
    "images/emotewheel1-400x400.jpg",
    "images/wedges.gif"
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "identity",
    "identity.email",
    "storage",
    "cookies",
    "https://accounts.google.com/",
    "https://accounts.google.com/o/oauth2/token"
  ],
  "oauth2": {
    "client_id": "246727289981-jrfoils1mgm35itpr1bigan57gh8nb1m.apps.googleusercontent.com",
    "scopes": ["profile"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon_16.png",
    "128": "images/icon_128.png"
  }
}
